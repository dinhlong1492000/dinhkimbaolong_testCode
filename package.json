    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "_moduleAliases": {
    "@n-services": "build/src/services",
    "@n-repositories": "build/src/repositories",
    "@n-utils": "build/src/utils",
    "@n-errors": "build/src/errors",
